{
  "name": "@mizdra/eslint-interactive",
  "description": "The CLI tool to run `eslint --fix` for each rule",
  "version": "6.0.0",
  "repository": "https://github.com/mizdra/eslint-interactive.git",
  "author": "mizdra <pp.mizdra@gmail.com>",
  "license": "MIT",
  "private": false,
  "type": "module",
  "exports": "./dist/index.js",
  "scripts": {
    "build": "tsc -p tsconfig.src.json",
    "dev": "tsc-watch -p tsconfig.src.json --onSuccess 'bin/eslint-interactive.js fixtures --rulesdir fixtures/rules --ext .js,.jsx,.mjs'",
    "lint": "run-s -c lint:*",
    "lint:tsc": "run-s -c lint:tsc:*",
    "lint:tsc:src": "tsc -p tsconfig.src.json --noEmit",
    "lint:tsc:test": "tsc -p tsconfig.test.json --noEmit",
    "lint:eslint": "eslint --ignore-pattern '/fixtures' .",
    "lint:prettier": "prettier --check .",
    "test": "NODE_OPTIONS=--experimental-vm-modules jest --selectProjects unit --colors",
    "postbuild:test": "yarn install --cwd e2e-test --frozen-lockfile && NODE_OPTIONS=--experimental-vm-modules jest --selectProjects e2e --colors"
  },
  "prettier": "@mizdra/prettier-config-mizdra",
  "renovate": "github>mizdra/renovate-config-mizdra",
  "packageManager": "yarn@1.22.15",
  "devDependencies": {
    "@jest/types": "^27.4.2",
    "@mizdra/eslint-config-mizdra": "^1.1.0",
    "@mizdra/prettier-config-mizdra": "^0.4.0",
    "@types/cli-table": "^0.3.0",
    "@types/eslint": "^8.2.1",
    "@types/estraverse": "^5.1.1",
    "@types/estree": "^0.0.50",
    "@types/jest": "^27.0.3",
    "@types/node": "^14.14.10",
    "@types/terminal-link": "^1.2.0",
    "@types/yargs": "^16.0.4",
    "@typescript-eslint/eslint-plugin": "^5.8.1",
    "@typescript-eslint/parser": "^5.8.1",
    "enhanced-resolve": "^5.8.3",
    "eslint": "^8.5.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.25.3",
    "jest": "^27.4.5",
    "jest-mock-process": "^1.4.1",
    "npm-run-all": "^4.1.5",
    "prettier": "2.5.1",
    "strip-ansi": "^7.0.1",
    "ts-jest": "^27.1.2",
    "tsc-watch": "^4.2.9",
    "typescript": "^4.6.0-dev.20211228"
  },
  "dependencies": {
    "boxen": "^6.2.1",
    "chalk": "^5.0.0",
    "cli-table": "^0.3.11",
    "comlink": "^4.3.1",
    "enquirer": "^2.3.6",
    "eslint-formatter-codeframe": "^7.32.1",
    "estraverse": "^5.3.0",
    "is-installed-globally": "^0.4.0",
    "node-pager": "^0.3.6",
    "ora": "^6.0.1",
    "source-map-support": "^0.5.20",
    "terminal-link": "^3.0.0",
    "yargs": "^16.2.0"
  },
  "peerDependencies": {
    "eslint": "^7.0.0 || ^8.0.0"
  },
  "engines": {
    "node": "^14.13.1 || >=16.0.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  },
  "bin": {
    "eslint-interactive": "bin/eslint-interactive.js"
  },
  "keywords": [
    "eslint"
  ],
  "files": [
    "bin",
    "src",
    "dist",
    "static"
  ]
}
